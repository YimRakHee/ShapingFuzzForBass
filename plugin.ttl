@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .

<https://github.com/YimRakHee>
    a foaf:Person ;
    foaf:name "YimRakHee" ;
    foaf:homepage <https://github.com/YimRakHee> .

<https://github.com/YimRakHee/ShapingFuzzForBass>
    a lv2:Plugin , lv2:FuzzPlugin ;
    doap:name "ShapingFuzzForBass" ;
    doap:license <https://opensource.org/licenses/MIT> ;
    doap:maintainer <https://github.com/YimRakHee> ;
    lv2:optionalFeature lv2:hardRTCapable ;
    lv2:port [
        a lv2:AudioPort ,
            lv2:InputPort ;
        lv2:index 0 ;
        lv2:symbol "in" ;
        lv2:name "In"
    ] , [
        a lv2:AudioPort ,
            lv2:OutputPort ;
        lv2:index 1 ;
        lv2:symbol "out" ;
        lv2:name "Out"
    ] , [
        a lv2:ControlPort ,
            lv2:InputPort ;
        lv2:index 2 ;
        lv2:symbol "drive" ;
        lv2:name "Drive" ;
        lv2:default 1.0 ;
        lv2:minimum 1.0 ;
        lv2:maximum 20.0 ;
    ] , [
        a lv2:ControlPort ,
            lv2:InputPort ;
        lv2:index 3 ;
        lv2:symbol "tone" ;
        lv2:name "Tone" ;
        lv2:default 2000.0 ;
        lv2:minimum 80.0 ;
        lv2:maximum 10000.0 ;
        units:unit units:hz ;
        lv2:portProperty lv2:logarithmic ;
    ] , [
        a lv2:ControlPort ,
            lv2:InputPort ;
        lv2:index 4 ;
        lv2:symbol "dry_lvl" ;
        lv2:name "Dry Level" ;
        lv2:default 0.7 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.5 ;
    ] , [
        a lv2:ControlPort ,
            lv2:InputPort ;
        lv2:index 5 ;
        lv2:symbol "wet_lvl" ;
        lv2:name "Wet Level" ;
        lv2:default 0.5 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.5 ;
    ] .
